[submodule "lib/pico-sdk"]
	path = hardware/firmware/lib/pico-sdk
	url = https://github.com/raspberrypi/pico-sdk
[submodule "freertos-smp"]
	path = hardware/firmware/lib/freertos-smp
	url = https://github.com/FreeRTOS/FreeRTOS-Kernel
	branch = smp
[submodule "tinyusb"]
	path = hardware/firmware/lib/tinyusb
	url = https://github.com/hathach/tinyusb
	branch = master
[submodule "cmake-git-version-tracking"]
	path = hardware/firmware/lib/cmake-git-version-tracking
	url = https://github.com/andrew-hardin/cmake-git-version-tracking
	branch = master
[submodule "pico-flashloader"]
	path = hardware/firmware/lib/pico-flashloader
	url = https://github.com/genoswitch/pico-flashloader
	branch = master
[submodule "lib/ringbuf"]
	path = hardware/firmware/lib/ringbuf
	url = https://github.com/genoswitch/c-ringbuf
